"UnlitGeneric"
{
	"$basetexture" "hud/leaderboard_class_soldier_burstfire_hyper_lite"
	"$vertexcolor" 1
	"$no_fullbright" 1
	"$ignorez" 1
	"%keywords" "tf"
	"$translucent" 1
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" "60"
		}
	}
}
